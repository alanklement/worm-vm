#
# use variable
#
# n = 5 + 6 / 3;
# write(n + 1);
# write(n + 2);

    # n
    SET    %S, $0

    # n = 5 + 6 / 7;
    SET    %A, $6
    SET    %B, $3
    DIV    %A, %B
    SET    %B, $5
    ADD    %A, %B
    STORE  @S, %A

    # write(n + 1)
    LOAD   %A, @S
    SET    %B, $1
    ADD    %A, %B
    WRITE

    # write(n + 2)
    ADD    %A, %B
    WRITE

